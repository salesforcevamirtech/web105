<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Accountnumber_lenth_should_not_be</fullName>
    <active>false</active>
    <errorConditionFormula>AND(NOT( ISNEW() ) , LEN( AccountNumber ) &gt;8)</errorConditionFormula>
    <errorMessage>Account number should not be 8 characters</errorMessage>
</ValidationRule>
